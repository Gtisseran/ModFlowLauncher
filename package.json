{
  "name": "modflow-launcher",
  "version": "1.0.0",
  "description": "Launcher Minecraft avec support CurseForge et Modrinth",
  "main": "src/main/main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder",
    "build:win": "electron-builder --windows"
  },
  "keywords": [
    "minecraft",
    "launcher",
    "modpack"
  ],
  "author": "Ton Nom",
  "license": "MIT",
  "devDependencies": {
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1"
  },
  "dependencies": {
    "@azure/msal-node": "^3.8.3",
    "adm-zip": "^0.5.16",
    "axios": "^1.6.2",
    "minecraft-launcher-core": "^3.18.2",
    "open": "^11.0.0"
  },
  "build": {
    "appId": "com.modflow.launcher",
    "productName": "ModFlow Launcher",
    "win": {
      "target": "nsis",
      "icon": "src/renderer/assets/icon.ico"
    }
  }
}
